{
  "expo": {
    "name": "dudulingo",
    "slug": "dudulingo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dudulingo.png",
    "scheme": "dudulingo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dudulingo.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dudulingo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.dudulingo.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-dudulingo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
